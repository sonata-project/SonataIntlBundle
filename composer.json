{
    "name": "sonata-project/intl-bundle",
    "type": "symfony-bundle",
    "description": "Symfony SonataIntlBundle",
    "keywords": ["sonata", "intl", "date", "time", "number"],
    "homepage": "https://sonata-project.org/bundles/intl",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Rabaix",
            "email": "thomas.rabaix@sonata-project.org"

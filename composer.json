    "keywords": ["sonata", "intl", "date", "time", "number"],
    "homepage": "http://sonata-project.org/bundles/intl",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Rabaix",
            "email": "thomas.rabaix@sonata-project.org"
        },
